{
  "name": "aap-conversion-homework",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://simzar.github.io/aap-conversion-fe-homework",
  "dependencies": {
    "customize-cra": "0.8.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "gh-pages": "2.1.1",
    "match-sorter": "4.0.2",
    "numeral": "^2.0.6",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-datepicker": "2.9.6",
    "react-dom": "16.11.0",
    "react-intl": "3.4.0",
    "react-redux": "7.1.1",
    "react-scripts": "3.2.0",
    "react-table": "6.10.3",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "node-sass": "4.13.0",
    "prettier": "1.18.2",
    "react-app-rewired": "2.1.4",
    "sass-resources-loader": "2.0.1"
  },
  "scripts": {
    "start": "npm run init-hooks && react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --coverage --watchAll=false",
    "test-int": "react-app-rewired test --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "init-hooks": "node scripts/verifyHook.js",
    "lint-js": "eslint --ext js,jsx --ignore-pattern src/**/*.d.js src/ --fix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
